# Geojb
# Search layers
source geojb_layers
{
	type = tsvpipe
	tsvpipe_command = cat ../../mapinfra/json/geojb/layers_search.tsv
	tsvpipe_field = layer
	tsvpipe_field = label
        tsvpipe_attr_string = layer
        tsvpipe_attr_string = label
}

index geojb_layers
{
    source 		= geojb_layers
    min_prefix_len      = 2
    path 		= /var/lib/sphinx/data/index/geojb_layers
}
